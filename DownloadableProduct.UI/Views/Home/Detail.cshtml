@using DownloadableProduct.Domain.Dto.Product
@model ProductDto
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detail</h2>
@Html.Partial("ErrorPartialView")
