@{
    ViewData["Title"] = "تایید کارت بانکی";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="bread">
    <div class="container">
        <ol class="breadcrumb">
            <li><a href="/">آی  گرافیک</a></li>
            <li><a href="/admin">پنل ادمین</a></li>
            <li class="active">تایید کارت بانکی</li>
        </ol>
    </div>
</div>

<div class="login">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="login__title">تایید کارت بانکی</div>
               
                <form method="post">
                    <input type="hidden" name="id" value="@ViewBag.Id"/>
                    @Html.AntiForgeryToken()
                    @Html.Partial("ErrorPartialView")
                   
                    <div class="form-group">
                        <label>  نام بانک  : </label>
                        <input type="text"  required name="bankName" class="form-control"/>
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn btn-success btn-block">تایید</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

